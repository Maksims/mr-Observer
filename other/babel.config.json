{
    "babelrc": false,
    "comments": false,
    "compact": false,
    "minified": false,
    "presets": [
        [
            "@babel/preset-env", {
                "bugfixes": true,
                "loose": true,
                "modules": false,
                "targets": {
                    "esmodules": true
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties", {
                "loose": true
            },
        ], [
            "babel-plugin-remove-import-export", {
                "removeExport": false
            }
        ]
    ]
}
